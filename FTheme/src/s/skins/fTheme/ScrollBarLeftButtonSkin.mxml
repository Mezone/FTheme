<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
	minWidth="{getStyle('scrollBarButtonSize')}" 
	minHeight="{getStyle('scrollBarSize')}"
	visible="{getStyle('scrollBarButtonSize') > 0}">
	
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata> 
	
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<s:Rect left="1" right="1" top="1" bottom="1"
		fill.up="{getStyle('buttonUpFill')}"
		fill.over="{getStyle('buttonOverFill')}"
		fill.down="{getStyle('buttonDownFill')}"
		fill.disabled="{getStyle('buttonDisabledFill')}"
		radiusX="{getStyle('scrollBarCornerRadius')}" topRightRadiusX="0" bottomRightRadiusX="0"/>
	
	<s:Rect left="0" right="0" top="0" bottom="0"
		stroke="{getStyle('scrollBarStroke')}"
		radiusX="{getStyle('scrrollBarCornerRadius')}" topRightRadiusX="0" bottomRightRadiusX="0"/>
	
	<s:Path horizontalCenter="-1" verticalCenter="0" id="arrow"
		data="M 0.0 3.5 L 7.0 0.0 L 7.0 7.0 L 0.0 3.5" fill="{getStyle('symbolFill')}"/>
	
</s:SparkSkin>
