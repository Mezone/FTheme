<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009">
	
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<s:Rect left="1" right="1" top="1" bottom="0" minWidth="12" minHeight="17"
		fill.up="{getStyle('buttonUpFill')}"
		fill.over="{getStyle('buttonOverFill')}"
		fill.down="{getStyle('buttonDownFill')}"
		fill.disabled="{getStyle('buttonDisabledFill')}"/>
	
	<s:Line left="0" right="0" bottom="-1"
		stroke="{getStyle('scrollBarStroke')}"/>
	
	<s:Path horizontalCenter="0" verticalCenter="-1" id="arrow"
		data="M 3.5 0.0 L 7.0 7.0 L 0.0 7.0 L 3.5 0.0" fill="{getStyle('symbolFill')}"/>

</s:SparkSkin>
